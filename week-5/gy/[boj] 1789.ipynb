   "source": [
    "N = int(input())\n",
    "s = 0\n",
    "cnt = 0\n",
    "num = 1\n",
    "while True:\n",
    "    s += num\n",
    "    cnt += 1\n",
    "    if s + num > N:\n",
    "        print(cnt)\n",
    "        break\n",
    "    num += 1"
   ]
